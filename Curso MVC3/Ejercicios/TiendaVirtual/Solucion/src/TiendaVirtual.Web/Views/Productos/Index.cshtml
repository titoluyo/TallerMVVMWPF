@model PagedList<TiendaVirtual.Domain.Producto>
@{
    ViewBag.Title = "Index";
}
<h2 class="center_title_bar">Productos</h2>
@foreach (var producto in Model)
{ 
    @Html.Partial("_producto",producto)
}
<div id="pagination">
    @Html.PageLinks(Model, i => Url.Action("index", new { pagina = i, categoria = ViewContext.RouteData.Values["categoria"] }))
</div>
